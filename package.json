{
  "name": "WireJS",
  "version": "0.0.1",
  "author": "Bruno Carneiro",
  "email": "brunocarneirofs@gmail.com",
  "description": "",
  "scripts": {
    "test": "NODE_PATH=$(pwd) ava client/**/__tests__/**/*.spec.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm run test && nyc report --reporter=html",
    "lint": "eslint ./src",
    "build": "webpack -p --progress --config webpack.prod.config.js && npm run post-build",
    "post-build": "sh ./scripts/post-build.sh",
    "mock": "node ./mocks/index",
    "start": "webpack-dev-server --hot --inline --history-api-fallback",
    "start:mock": "npm run mock & BASE_URL=http://localhost:3000/ node server.js",
    "deploy:qa": "sudo sh ./scripts/deploy.qa.sh"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react": "^1.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.8.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "glamor": "^2.20.24",
    "glamorous": "^3.8.0",
    "html-webpack-plugin": "^2.22.0",
    "js-cookie": "^2.1.3",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "node-sass": "^4.5.0",
    "rc-progress": "^2.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.3.2",
    "react-intl": "^2.1.3",
    "react-router": "^4.1.1",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.16.1",
    "webpack": "2.4.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-eslint": "^7.2.2",
    "babel-register": "^6.24.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "hapi": "^16.1.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "nyc": "^10.2.2",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.1.0",
    "webpack-dev-server": "^2.4.2"
  },
  "ava": {
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  }
}
