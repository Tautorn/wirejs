{
  "name": "WireJS",
  "version": "0.0.1",
  "author": "Bruno Carneiro",
  "email": "brunocarneirofs@gmail.com",
  "description": "",
  "scripts": {
    "test": "NODE_PATH=$(pwd) ava client/**/__tests__/**/*.spec.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm run test && nyc report --reporter=html",
    "lint": "eslint ./src",
    "build": "webpack -p --progress --config webpack.prod.config.js && npm run post-build",
    "post-build": "sh ./scripts/post-build.sh",
    "mock": "node ./mocks/index",
    "start": "webpack-dev-server --hot --inline --history-api-fallback",
    "start:mock": "npm run mock & BASE_URL=http://localhost:3000/ node server.js",
    "deploy:qa": "sudo sh ./scripts/deploy.qa.sh"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react": "^1.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.8.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "glamor": "^2.20.24",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^4.5.0",
    "react": "^15.5.3",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.4",
    "react-router": "^2.4.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "styled-components": "^1.4.4",
    "webpack": "2.2.0"
  },
  "devDependencies": {
    "ava": "^0.18.0",
    "babel-eslint": "^7.1.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.2.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jasmine": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "hapi": "^15.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.8.3",
    "nyc": "^10.0.0",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.0.2",
    "react-test-renderer": "^15.3.2",
    "sinon": "^2.0.0-pre.2",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "ava": {
    "require": [
      "babel-register",
      "ignore-styles",
      "./scripts/unit-test-setup.js"
    ],
    "babel": "inherit"
  }
}
